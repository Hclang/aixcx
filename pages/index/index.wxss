  justify-content: space-between;
  align-items: center;
} 

.frame_item{
  width:250rpx;
  height:250rpx;
  background-color: cornsilk;
  display: flex;
  justify-content: center;  
  align-items: center;   
}

.frame_item_top{
  width:580rpx;
  height:250rpx;
